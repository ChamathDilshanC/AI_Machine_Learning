# Configuration for Pipeline Deployment (07_Pipeline_Deployment)
# Copy this file to .env and fill in your actual values

# Your Google Cloud Project ID
GCP_PROJECT_ID=your-project-id-here

# Google Cloud Region (e.g., us-central1, asia-southeast1)
GCP_REGION=us-central1

# Google Cloud Storage Bucket Name (without gs:// prefix)
# This bucket will store your model files
GCS_BUCKET_NAME=your-bucket-name

# Model Display Name (name for your model in Vertex AI)
MODEL_DISPLAY_NAME=sentiment-pipeline-model

# Endpoint Display Name (name for your endpoint)
ENDPOINT_DISPLAY_NAME=sentiment-endpoint

# Local model file path
LOCAL_MODEL_PATH=model.joblib

# Optional: Path to service account key (if not using gcloud auth)
# GOOGLE_APPLICATION_CREDENTIALS=path/to/key.json
